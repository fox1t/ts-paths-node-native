{
  "name": "ts-paths-node-native",
  "version": "1.0.0",
  "description": "TypeScript paths with Node.js without any addtional bundling/transformation step.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist"
  },
  "imports": {
    "#subpath/*": {
      "require": "./dist/subpath/*.js"
    }
  },
  "engines": {
    "node": ">=12.20.0 <13.0.0 || >=14.13.0 <15.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fox1t/ts-paths-node-native.git"
  },
  "author": "Maksim Sinik <maksim@sinik.it>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fox1t/ts-paths-node-native/issues"
  },
  "homepage": "https://github.com/fox1t/ts-paths-node-native#readme",
  "devDependencies": {
    "@types/node": "^14.14.10",
    "fastify-tsconfig": "^1.0.1",
    "typescript": "^4.1.2"
  }
}